{
  "name": "vscode-jest-debugging-presentation",
  "version": "1.0.0",
  "description": "Sample React App",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config ./config/webpack/dev.js",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./config/webpack/prod.js",
    "build:dev": "npm run clean && webpack --config ./config/webpack/dev.js",
    "analyze": "npm run clean && webpack --config ./config/webpack/analyze.js",
    "test": "jest -c ./config/test/jest.json --verbose",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i",
    "test:coverage": "rimraf coverage && jest -c ./config/test/jest.coverage.json --verbose -i"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "core-js": "^3.41.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.0.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/redux-mock-store": "^1.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "raf": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
